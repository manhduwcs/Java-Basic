package myPackage1;

import java.util.Scanner;

public class FirstClass {
	public static void main(String[] args) {
		int n;
		int i = 0;
		Scanner sc = new Scanner(System.in);
		System.out.print("N = ");
		n = sc.nextInt();
		
		while(i<=n) {
			if(i<=(int)(n/2)) {
				if(i%2==0) {
					System.out.println("Even "+i);
					i++;
				}
				else {
					i++;
					continue;					
				}
			}
		}
	}
}
