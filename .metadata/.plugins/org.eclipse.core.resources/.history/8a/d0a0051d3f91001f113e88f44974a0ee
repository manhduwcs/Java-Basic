package truePackage1;

public class StaticAndNormalVariable {
	static int st_num1 = 11;
	int num2 = 10;

	public int counter(int number) {
		return number + 1;
	}

	public static void main(String[] args) {
		StaticAndNormalVariable counter1 = new StaticAndNormalVariable();
		StaticAndNormalVariable counter2 = new StaticAndNormalVariable();

		counter1.counter(st_num1);

		counter2.counter(st_num1);

	}
}
